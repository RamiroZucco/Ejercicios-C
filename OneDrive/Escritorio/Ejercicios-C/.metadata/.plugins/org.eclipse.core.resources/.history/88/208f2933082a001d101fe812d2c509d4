#include <stdio.h>
#include <stdlib.h>
int Restar1(int,int);
int Restar2(void);

int main() {
	int primerNumero;
	int segundoNumero;
	int resultado;
	scanf("%d",&primerNumero);
	scanf("%d",&segundoNumero);
	resultado=Restar1(primerNumero,segundoNumero);
	printf("El resultado de la resta es %d",resultado);

	resultado=Restar2();
	printf("El resultado de la resta es %d",resultado);
}
int Restar1(int primerNumero, int segundoNumero)
{
	int resultado;
	resultado=primerNumero-segundoNumero;
	return resultado;
}
int Restar2(void)
{
	int resultado;
	int primerNumero;
	int segundoNumero;
	resultado=primerNumero-segundoNumero;
}
