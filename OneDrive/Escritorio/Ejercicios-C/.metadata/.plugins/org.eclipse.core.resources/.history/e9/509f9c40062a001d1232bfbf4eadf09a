#include <stdio.h>
#include <stdlib.h>
int Restar1(int,int);
int main() {
	int primerNumero;
	int segundoNumero;
	int resultado;
	printf("Ingrese un numero");
	scanf("%d",&primerNumero);
	printf("Ingrese un numero");
	scanf("%d",&segundoNumero);
	resultado=Restar1(primerNumero,segundoNumero);
	printf("%d",resultado);
}
int Restar1(int primerNumero, int segundoNumero)
{
	int resultado;
	resultado=primerNumero-segundoNumero;
	return resultado;
}
